<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dsoft.myrestaurant" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.MyRestaurant" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".FinanzasActivity"
            android:label="Finanzas"
            android:parentActivityName="com.dsoft.myrestaurant.MyActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dsoft.mycalendar.MyActivity" />
        </activity>


    </application>

</manifest>
